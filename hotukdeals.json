{
    "name": "reddit",
    "baseUrl": "http://www.hotukdeals.com/",
    "nextUrlDescriptor": {
        "selector": ".redesign-pagination .p-next a",
        "extract": "href"
    },
    "itemTypes": [{
        "name": "deal",
        "container": ".s-items-listings",
        "selector": ".redesign-item-listing",
        "exclude": ".newsletter-subscription-inline",
        "properties": {
            "title": "a.title",
            "temperature": ".voting-controls .temperature"
        },
        "template": "{{ temperature }} {{ title }}"
    }],
    "sanitizers": ["trim"]
}
